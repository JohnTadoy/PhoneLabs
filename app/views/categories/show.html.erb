<div class = "container grid text-center">
<%@show.each do |m|%>
<div class="g-col-3 card shadow p-3 mb-5 bg-body rounded text-center">
  <h1><a href="<%= mobile_phone_path(m.id) %>"><%= m.name %></a></h1>
  <img src="<%= m.image %>" alt="<%= m.name %>">
  <p>$<%= m.price%></p>
  <p><%= m.description %></p>
  </div>
  <%end%>
</div>